    _leftCharacterTalking: 1
    _narratorSpeaking: 0
  - _character: {fileID: 11400000, guid: 4ef67433d0f3fb44ea72e0859c818f67, type: 2}
    _characterTwo: {fileID: 11400000, guid: 7f749282c9edb3245bcb661e4107fc11, type: 2}
    _hasPoseIndex: 1
    _hasSecondCharacterPoseIndex: 1
    _poseIndex: 12
    _poseIndexTwo: 0
    _text: 'She was picked to take her trial soon after I was born. '
